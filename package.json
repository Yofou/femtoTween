{
  "name": "femtotween",
  "version": "1.0.1",
  "description": "a minimalistic tween library",
  "repository": {
    "type": "git",
    "url": "https://github.com/pearofducks/femtoTween"
  },
  "main": "dist/femtoTween.js",
  "module": "dist/femtoTween.esm.js",
  "sideEffects": false,
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ava",
    "build": "rollup -c rollup.config.js",
    "prebump": "npm run build",
    "bump": "npm version patch",
    "version": "npm run build && npm publish --access public"
  },
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.13.8",
    "ava": "^1.4.1",
    "rollup": "^1.10.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^7.3.2",
    "ts-node": "^8.1.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5",
    "window": "^4.2.6"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
