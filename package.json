{
  "name": "femtotween",
  "version": "1.0.2",
  "description": "a minimalistic tween library",
  "repository": {
    "type": "git",
    "url": "https://github.com/pearofducks/femtoTween"
  },
  "main": "dist/femtoTween.umd.js",
  "module": "dist/femtoTween.esm.js",
  "sideEffects": false,
  "scripts": {
    "test": "ava",
    "build": "rollup -c rollup.config.js",
    "bump": "npm version patch",
    "version": "npm run build && npm publish --access public",
    "postversion": "git push --follow-tags"
  },
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.4.1",
    "rollup": "^1.13.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "sinon": "^7.3.2",
    "window": "^4.2.6"
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "require": [
      "esm"
    ],
    "verbose": true
  }
}
