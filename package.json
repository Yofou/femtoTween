{
  "name": "femtotween",
  "version": "0.9.0",
  "description": "a minimalistic tween library",
  "main": "dist/femtoTween.js",
  "module": "dist/femtoTween.esm.js",
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ava",
    "build": "rollup -c rollup.config.js",
    "prebump": "npm run build",
    "bump": "npm version patch",
    "version": "npm run build && npm publish --access public"
  },
  "author": "Dave Honneffer <pearofducks@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^10.11.3",
    "ava": "^1.0.0-beta.8",
    "rollup": "^0.66.6",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript": "^1.0.0",
    "sinon": "^7.1.1",
    "ts-node": "^7.0.1",
    "tslib": "^1.9.3",
    "typescript": "^3.1.6",
    "window": "^4.2.5"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
